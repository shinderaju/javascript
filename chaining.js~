var calculation = {
   num1 : 0,
   num2 : 0,
   first :function (num){
    this.num1 = num;
    return this;
  },
  second : function (num){
    this.num2 = num;
    return this;
  },
  sum : function(){
    var add = this.num1 + this.num2;
    document.write("sum is "+add+"<br/>");
    return add;
  }
};
calculation.first(1).second(2).sum();
